{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "android24",
            "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
            "args": [
                "-u",
                "tdd",
                "--timeout",
                "999999",
                "--colors",
                "--runType",
                "android24",
                "${workspaceRoot}/e2e",
                "-v"
            ],
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}